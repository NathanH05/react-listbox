{
  "name": "react-listbox",
  "version": "0.0.0-semantic-release",
  "description": "a double listbox react component",
  "main": "build/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha --compilers js:babel-core/register --recursive tests/**/*.spec.js --require tests/setup.js",
    "prebuild": "rimraf build && rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir build src",
    "build:umd": "webpack --output-filename react-listbox.js",
    "build:umd.min": "webpack --output-filename react-listbox.min.js -p",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sridatta19/react-listbox.git"
  },
  "keywords": [
    "react",
    "react-component",
    "listbox",
    "multi-select"
  ],
  "files": [
    "build",
    "public"
  ],
  "author": "Sridatta <dattacrew19@gmail.com> (https://twitter.com/dattacrew19)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sridatta19/react-listbox/issues"
  },
  "homepage": "https://github.com/Sridatta19/react-listbox#readme",
  "dependencies": {
    "classnames": "2.2.5",
    "ramda": "0.22.1"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0.0-rc || ^15.0",
    "react-dom": "^0.14 || ^15.0.0-rc || ^15.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.16.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "chai": "3.5.0",
    "commitizen": "2.8.6",
    "css-loader": "0.25.0",
    "cz-conventional-changelog": "1.2.0",
    "enzyme": "2.4.1",
    "eslint": "3.8.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.20.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "ghooks": "1.3.2",
    "ignore-styles": "5.0.1",
    "json-loader": "0.5.4",
    "mocha": "3.1.2",
    "npm-run-all": "3.1.1",
    "nyc": "8.3.1",
    "postcss-loader": "0.13.0",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
